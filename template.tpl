const log = require('logToConsole');
log('data =', data);

const copyFromDataLayer = require('copyFromDataLayer');
const injectScript = require('injectScript'); 
const url = 'https://cdn.storepredictor.com/sp.v2.js';

injectScript(url, (sp) => {
  sp('create', data.code, 'auto');
  sp('send', 'pageview', '');
  if (data.productCode.length > 2) {
    sp('send', 'pageview', data.productCode);
  }
  
  if (data.orderNumber.length > 0) {
    sp('send', 'order', { orderId: data.orderNumber, items: data.items, totalPrice: data.totalPrice, currency: data.currency });
  }
  
}, () => log('Faile to load'));

data.gtmOnSuccess();


